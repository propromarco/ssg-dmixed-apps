<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='clubbiertest_gwt_app'>

  <inherits name='com.google.gwt.user.User' />
  <inherits name='com.google.gwt.json.JSON' />
  <inherits name='com.google.gwt.i18n.I18N' />
  <inherits name='com.google.gwt.place.Place' />
  <inherits name="com.google.msc.framework.framework"/>
  <inherits name="de.clubbiertest.liste.Liste"/>

  <inherits name='com.sksamuel.Jqm4gwt' />
  <entry-point class='de.clubbiertest.client.ClubbiertestJqm4gwt' />

  <source path='client' />
  <source path='shared' />

  <extend-property name="locale" values="de" />
  <extend-property name="locale" values="en" />

  <set-property-fallback name="locale" value="de" />

  <set-property name="user.agent" value="gecko1_8,safari,opera"/>
</module>
